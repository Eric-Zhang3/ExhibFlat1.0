/*CSS RESET 共用*/
body{
	margin:0;
	padding:0;
	font-size:12px;
	color:#666;
	font-family:"Microsoft YaHei","Arial";
	background: #fff;
}
form,ul,li,ol,li,dl,dt,dd,img,p,span,div,h1,h2,h3,h4,h5,h6,input{
	margin:0;
	padding:0;
}
input,select{
	font-size:12px;
	outline: none;
	font-family: "Microsoft YaHei";
}
img{
	border:0;
}
ul,li{
	list-style-type:none;
}
strong{
	font-weight:normal;
}
em,i,cite{
	font-style:normal;
}
h1,h2,h3,h4,h5,h6 {
	font-weight:normal;
	font-size: 12px;
}
a{
	color:#797979;
	text-decoration:none;
}
a:hover{
	color:#e81250;
	text-decoration:underline;
}
a:focus,li:focus{
	outline: none;
}
table{
	width: 100%;
	border-collapse: collapse;
}
.fr{
	float: right;
}
.fl{
	float: left;
}
.textC{
	text-align: center;
}
.siteNav li .fav,.siteNav li .sHelp,.siteNav li .arrow,.siteNav li .car,.siteNav li .list,
.addList ul li h2 i,.flex-direction-nav li a,.flex-control-nav a,.banNum span,.member p a i,.memberL li a em,.proBtn span
,.hotType h2,.hotType ul li b,.hotType ul li em,.brandList span,.sickSearch .sickBtn,.rank ul li span i,.scrollDiv i,.searchHot .search .sub
,.cateMenu ul li h2 i,.cateMenu ul li h2 em,.cateMenu .siftBg a i,.navList ul .myOrder b,.navList ul .jsc i
{
