}
.navList ul .myOrder a{
	padding: 0;
	font-size: 13px;
	font-weight: normal;
}
.navList ul .myOrder i{
	display: inline-block;
	*display: inline;
	*zoom: 1;
	width: 41px;
	height: 37px;
