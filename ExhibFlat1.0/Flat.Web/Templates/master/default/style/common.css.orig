}
.navList{
    width:1200px;
    margin:0 auto;
    height: 37px;
    line-height: 37px;
    z-index:9;        
}
.navList ul li {
    float: left;
    text-align: center; 
	position:relative;       
}
.navList ul li img{
	float: left;
}
.navList ul li span{
	visibility: hidden;
}
.navList ul li a:hover span{
