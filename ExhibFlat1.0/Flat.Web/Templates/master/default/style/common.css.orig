/*新增分页控件*/
.pagination{
  float:right;
  margin: 15px 0;
}
.pagination .page-skip{
  color:#999;
  float:left;
  height:28px;
  line-height:26px;
  overflow:hidden;
  padding:0 5px;
  margin: -2px 5px 0 0;
  text-align:center;
  white-space:nowrap;
  font-size:13px;
  vertical-align:middle;
}
.pagination span{
  display:block;
  float:left;
  padding:0 5px;
  height:21px;
}
.pagination span.page-cur{
  line-height:21px;
  height:21px;
  border:#d1d1d1 1px solid;
  text-align:center;
  margin-right:3px;
  color:#616c75;
  font-weight:bold;
}
.pagination a{
  display:block;
  float:left;
  height:21px;
  padding:0 5px;
  line-height:21px;
  background:white;
  text-align:center;
  margin-right:3px;
  color:#616c75;
  border: 1px solid #c6c6c6;
}
.pagination a:hover{
  border:#d1d1d1 1px solid;
  color:#616c75;
  text-decoration:none;
}
.pagination a.page-prev{
  background:url(../images/process/icons.gif) white no-repeat 4px -235px;
  padding-left:12px;
}
.pagination a.page-next{
  font-weight:normal;
  background:url(../images/process/icons.gif) white no-repeat right -248px;
  padding-right:12px;
}
.pagination .text{
  border:#a7a6aa 1px solid;
  color:#333;
  width:25px;
  padding:2px;
  margin:0px 3px;
}
.pagination .button{
  width:52px;
  height:22px;
  line-height:22px;
  border:none;
  margin:0px 5px;
  background: #fff;
  border: 1px solid #a7a6aa;
  cursor: pointer;
}


