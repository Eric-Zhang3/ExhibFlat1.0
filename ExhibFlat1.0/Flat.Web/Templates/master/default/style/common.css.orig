}

.cartList{
    width: 175px;
    background: #fff;
    z-index: 14;
    position: absolute;
    right: 0;
    text-align: left;
    display: none;
	box-shadow: 1px 2px 4px rgba(0,0,0,.2);
  	-webkit-box-shadow: 1px 2px 4px rgba(0,0,0,.2);
  	-moz-box-shadow: 1px 2px 4px rgba(0,0,0,.2);  
}
.cartList h2{
    text-align: center;
}
.cartInner{
    padding: 0 10px 15px;   
}
.cartInner p{
    color: #3e3e3e;
}
.cartInner .checkOrder{
    text-align: center;
