}
.cartInner dl dd strong{
    color: #f22e00;
    font-weight: bold;
    font-size: 14px;
}
.cartInner dl dd strong em{
    font-weight: normal;
    color: #767676;
    font-size: 12px;
}
.cartInner dl dd p a{
    color: #767676;
    text-align: right;
    font-size: 12px;
    font-weight: normal;
}
